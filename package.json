{
  "name": "chat",
  "version": "2.0.5",
  "main": "dist-electron/main/index.js",
  "description": "一个聊天工具客户端",
  "author": "Jokul <986432015@qq.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "buildDEB": "vue-tsc --noEmit && vite build && electron-builder --linux deb",
    "buildWIN": "vue-tsc --noEmit && vite build && electron-builder --windows nsis:x64",
    "buildMAC": "vue-tsc --noEmit && vite build && electron-builder --mac",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@iconify-json/ep": "^1.1.10",
    "@types/event-source-polyfill": "^1.0.1",
    "@vitejs/plugin-vue": "^4.1.0",
    "electron": "^24.1.3",
    "electron-builder": "^24.4.0",
    "typescript": "^5.0.2",
    "unplugin-auto-import": "^0.16.1",
    "unplugin-icons": "^0.16.1",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.1.4",
    "vite-plugin-electron": "^0.11.2",
    "vite-plugin-electron-renderer": "^0.14.1",
    "vue": "^3.2.47",
    "vue-tsc": "^1.2.0"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@types/markdown-it": "^12.2.3",
    "axios": "^1.4.0",
    "element-plus": "^2.3.5",
    "event-source-polyfill": "^1.0.31",
    "highlight.js": "^11.8.0",
    "markdown-it": "^13.0.1",
    "pinia": "^2.1.3",
    "vue-router": "^4.2.1"
  }
}
